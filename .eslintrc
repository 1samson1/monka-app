{
    "parser": "@babel/eslint-parser",
    "parserOptions" : {
        "requireConfigFile": false,
        "ecmaVersion": 8
    },
    "rules": {
        "no-unused-vars": "warn"
    },
    "env": {
        "es6": true,
        "browser": true
    },
    "extends": [
        "plugin:vue/vue3-recommended",
        "eslint:recommended"
    ],
    "globals": {
        "PRODUCTION": false
    }
}
